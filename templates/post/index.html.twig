<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="{{ asset('css/forum.css') }}">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css">
    <title>Forum - Page d'accueil</title>
</head>
<body>
<div class="container">
    <!-- Barre latÃ©rale gauche -->
    <div class="left-sidebar">
        <div class="logo">TCG LPRS</div>
        <button class="active" onclick="changerPage('')"><i class="fas fa-home"></i> Accueil</button>
        <button onclick="changerPage('entreprise')"><i class="fas fa-building"></i> Entreprises</button>
        <button onclick="changerPage('etudiants')"><i class="fas fa-users"></i> Etudiants</button>
        <button onclick="changerPage('alumnis')"><i class="fas fa-user-graduate"></i> Alumnis</button>
        <button onclick="changerPage('professeur')"><i class="fas fa-chalkboard-teacher"></i> Professeurs</button>
        <button onclick="changerPage('mes_posts')"><i class="fas fa-pencil-alt"></i> Mes Posts</button>
        <button onclick="changerPage('profil')"><i class="fas fa-user"></i> Profil</button>
    </div>

    <!-- Contenu principal -->
    <div class="main-content">
        <div class="feed-content">
            <!-- Formulaire de publication -->
            <div class="create-indication">
                <img src="data:image/jpeg;base64," alt="Votre photo de profil">

                {{ form_start(form) }}
                <div class="form-group">
                    {{ form_row(form.titre) }}        <!-- Titre -->
                </div>
                <div class="form-group">
                    {{ form_row(form.contenuPost) }}   <!-- Contenu du post -->
                </div>
                <div class="form-group">
                    {{ form_row(form.canal) }}         <!-- Canal -->
                </div>
                <button type="submit" class="btn-publish">Publier</button>   <!-- Bouton pour soumettre le formulaire -->
                {{ form_end(form) }}
            </div>

            <!-- Affichage des posts -->
            {% for post in posts %}
                <div class="indication">
                    <div class="indication-header">
                        <img src="data:image/jpeg;base64," alt="Avatar de l'utilisateur">
                        <div class="user-info">
                            <!-- Suppression de la rÃ©fÃ©rence Ã  l'utilisateur -->
                            <!-- <h3>{{ post.refUtilisateur.nom }}</h3> -->
                        </div>
                    </div>
                    <h2>{{ post.titre }}</h2>
                    <p>{{ post.contenuPost }}</p>

                    {% for reponse in post.reponses %}
                        <div class="reponse">
                            <p>{{ reponse.contenu }}</p>
                        </div>
                    {% endfor %}

                    <div class="indication-actions">
                        <button onclick="afficherCommentaires({{ post.id }})"><i>ð¬</i> Commenter</button>
                    </div>
                </div>
            {% else %}
                <p>Aucun post trouvÃ©.</p>
            {% endfor %}
        </div>
    </div>

    <!-- Barre latÃ©rale droite -->
    <div class="right-sidebar">
        <div class="right-sidebar-section">
            <h2>DerniÃ¨res Entreprises</h2>
        </div>
    </div>
</div>
</body>
<script>
    function changerPage(page) {
        window.location.href = '/' + page;
    }
</script>

</html>
